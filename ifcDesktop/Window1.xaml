<Window x:Class="ifcDesktop.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ifcDesktop"
        mc:Ignorable="d"
        Title="更改名单" Height="483" Width="379" Loaded="WinLoaded" ResizeMode="NoResize">
    <Grid Margin="10,0,0,9">
        <ComboBox x:Name="Cb1" HorizontalAlignment="Left" Margin="10,389,0,0" VerticalAlignment="Top" Width="232"/>
        <Button Content="添加" HorizontalAlignment="Left" Margin="261,391,0,0" VerticalAlignment="Top" Width="75" FontSize="11" Click="Bt_Click_Add"/>
        <TextBox x:Name="TextView" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="327" Height="374" IsEnabled="False" MaxWidth="327" MaxHeight="374" />
        <ComboBox x:Name="Cb2" HorizontalAlignment="Left" Margin="10,415,0,0" VerticalAlignment="Top" Width="232"/>
        <Button Content="删除" HorizontalAlignment="Left" Margin="261,417,0,0" VerticalAlignment="Top" Width="75" FontSize="11" Click="Bt_Click_rme"/>
    </Grid>
</Window>
